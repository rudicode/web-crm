
    <h1><i class="fi-list"></i>List All Contacts</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th>First</th>
      <th>Last</th>
      <th>Email</th>
      <th>Notes</th>
      <th></th>
    </tr>
  </thead>
    <% @contacts.each do |contact| %>
    <tr>
        <td><a href="/contacts/edit/<%= contact.id %>"><i class="fi-page-edit"></i></a></td>
        <td><%= contact.first_name %></td>
        <td><%= contact.last_name %></td>
        <td><%= contact.email %></td>
        <td><%= contact.notes %></td>
        <td><a href="/contacts/delete/<%= contact.id %>"><i class="fi-x"></i></a></td>
    </tr>
    <% end %>
  </tbody>
</table>

